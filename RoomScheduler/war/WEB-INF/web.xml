<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Assignment2</servlet-name>
		<servlet-class>pc.RootServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Assignment2</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Booking</servlet-name>
		<servlet-class>pc.BookingServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Booking</servlet-name>
		<url-pattern>/booking</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>viewResults</servlet-name>
		<servlet-class>pc.ViewResultsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>viewResults</servlet-name>
		<url-pattern>/viewResults</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>deleteBooking</servlet-name>
		<servlet-class>pc.DeleteBookingServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>deleteBooking</servlet-name>
		<url-pattern>/deleteBooking</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>filterView</servlet-name>
		<servlet-class>pc.FilterViewServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>filterView</servlet-name>
		<url-pattern>/filterView</url-pattern>
	</servlet-mapping>
	
</web-app>
